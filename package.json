{
  "name": "aah",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite --open",
    "local": "vite serve --mode local",
    "dev": "vite serve --mode dev --port 3001 --host",
    "stg": "vite serve --mode stg",
    "prod": "vite serve --mode prod",
    "build:dev": "vite build --mode dev && git rev-parse --short HEAD > dist/version.txt",
    "build:stg": "vite build --mode stg && git rev-parse --short HEAD > dist/version.txt",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod && git rev-parse --short HEAD > dist/version.txt",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "postinstall": "patch-package"
  },
